{
"compilerOptions": {
"module": "commonjs",
"target": "es6",
"noImplicitAny": false,
"moduleResolution": "node",
"allowSyntheticDefaultImports": true,
"resolveJsonModule": true,
"esModuleInterop": false,
"allowJs": true,
"checkJs": false,
"sourceMap": true,
"outDir": "./dist",
"baseUrl": ".",
"skipLibCheck": true,
"paths": {
"*": [
"node_modules/*",
"src/types/*"
]
}
},
"exclude": ["dist"],
"include": [
"extraTs","src"
, "index.ts"],
"types": ["src/types"]
}